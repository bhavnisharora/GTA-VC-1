{
    "name": "Grand Theft Auto Vice City",
    "colors": {
        "editor.background": "#2c2b2a",
        "editor.foreground": "#ffffff",
        "editor.selectionBackground": "#524949",
        "editor.selectionHighlightBackground": "#5a534f",
        "editorLineNumber.foreground": "#adadad",
        "editorLineNumber.activeForeground": "#ffffff",
        "editorCursor.foreground": "#ffffff",
        "editor.lineHighlightBackground": "#383533",
        "activityBar.background": "#a79c8e",
        "activityBar.foreground": "#ffffff",
        "activityBarBadge.background": "#ff0000",
        "activityBarBadge.foreground": "#ffffff",
        "sideBar.background": "#2c2b2a",
        "sideBar.foreground": "#ffffff",
        "sideBarSectionHeader.background": "#524949",
        "sideBarSectionHeader.foreground": "#ffffff",
        "list.hoverBackground": "#524949",
        "list.focusBackground": "#5a534f",
        "statusBar.background": "#a79c8e",
        "statusBar.foreground": "#ffffff",
        "statusBar.noFolderBackground": "#a79c8e",
        "statusBar.noFolderForeground": "#ffffff",
        "titleBar.activeBackground": "#a79c8e",
        "titleBar.activeForeground": "#ffffff",
        "titleBar.inactiveBackground": "#2c2b2a",
        "titleBar.inactiveForeground": "#ffffff"
    },
    "tokenColors": [
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "foreground": "#bdc2c8"
            }
        },
        {
            "name": "String",
            "scope": "string",
            "settings": {
                "foreground": "#ffa500"
            }
        },
        {
            "name": "Keyword",
            "scope": "keyword",
            "settings": {
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Number",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#d0a030"
            }
        },
        {
            "name": "Function",
            "scope": "entity.name.function",
            "settings": {
                "foreground": "#ffffff"
            }
        }
    ],
    "semanticHighlighting": true,
    "colorschemes": {
        "headerForeground": "#ffffff",
        "headerBackground": "#a79c8e",
        "borderColor": "#a79c8e",
        "dropdownBackground": "#2c2b2a",
        "dropdownBorder": "#a79c8e",
        "dropdownForeground": "#ffffff"
    },
    "iconTheme": {
        "path": "/path/to/rockstar.png"
    }
}
